<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teacher.study_t" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LifecycleActivity"
            android:label="@string/title_activity_lifecycle" >
        </activity>
        <activity
            android:name=".AsyncActivity"
            android:label="@string/title_activity_async" >
        </activity>

        <service
            android:name=".StartService"
            android:exported="false" >
        </service>

        <activity
            android:name=".ServiceActivity"
            android:label="@string/title_activity_service" >
        </activity>
    </application>

</manifest>
